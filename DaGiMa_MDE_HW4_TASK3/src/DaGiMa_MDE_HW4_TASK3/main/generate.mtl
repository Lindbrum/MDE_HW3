[comment encoding = UTF-8 /]
[module generate('http://www.example.org/dagima_mde_hw4_dest')]
[import DaGiMa_MDE_HW4_TASK3::files::university /]
[import DaGiMa_MDE_HW4_TASK3::files::department /]
[import DaGiMa_MDE_HW4_TASK3::files::degreeCourse /]
[import DaGiMa_MDE_HW4_TASK3::files::professor /]
[import DaGiMa_MDE_HW4_TASK3::files::news /]
[import DaGiMa_MDE_HW4_TASK3::files::course /]
[import DaGiMa_MDE_HW4_TASK3::common::htmlHead /]

[template public generateIndex(aContainer : Container)]
[comment @main/]
[file ('index.html', false, 'UTF-8')]
<!DOCTYPE html>
<html lang="en">
<head>
[generateHead(aContainer.model_name) /]
</head>
<body>
<main class="container">
[comment ------------------------------------  /]
<p>
<h2>List of Universities</h2>
<table class="table table-bordered">
<thead>
<tr>
<th>Name</th>
<th>URL</th>
</tr>
</thead>
<tbody>
[for(uni : University | aContainer.universities)] 
[uni.generateUniversity() /]
<tr>
<td> 
[uni.name/]
</td>
<td>
<a href="[uni.name/].html">link</a>
</td>
</tr>
[/for]
</tbody>
</table>
</p>
[comment ------------------------------------  /]
<p>
<h2>List of Departments</h2>
<table class="table table-bordered">
<thead>
<tr>
<th>Name</th>
<th>URL</th>
</tr>
</thead>
<tbody>
[for(uni : University | aContainer.universities)] 
[for(dep : Department | uni.departments)]
[dep.generateDepartment() /]
<tr>
<td> 
[dep.name/]
</td>
<td>
<a href="[dep.name/]-dep.html"> link </a>
</td>
</tr>
[/for]
[/for]
</tbody>
</table>
</p>
[comment ------------------------------------  /]
<p>
<h2>List of DegreeCourses</h2>
<table class="table table-bordered">
<thead>
<tr>
<th>Name</th>
<th>URL</th>
</tr>
</thead>
<tbody>
[for(uni : University | aContainer.universities)] 
[for(dep : Department | uni.departments)]
[for(deg : DegreeCourse | dep.offered_courses)]
[deg.generateDegreeCourse() /]
<tr>
<td> 
[deg.name/]
</td>
<td>
<a href="[deg.name/]-deg.html"> link </a>
</td>
</tr>
[/for]
[/for]
[/for]
</tbody>
</table>
</p>

[comment ------------------------------------  /]
<p>
<h2>List of Courses</h2>
<table class="table table-bordered">
<thead>
<tr>
<th>Course name</th>
<th>URL</th>
</tr>
</thead>
<tbody>
[for(cor : Course | aContainer.courses)] 
[cor.generateCourse() /]
<tr>
<td> 
[cor.name/]
</td>
<td>
<a href="[cor.name/]-cor.html">link</a>
</td>
</tr>
[/for]
</tbody>
</table>
</p>

[comment ------------------------------------  /]
<p>
<h2>List of Examination Calls</h2>
<table class="table table-bordered">
<thead>
<tr>
<th>Course code</th>
<th>URL</th>
</tr>
</thead>
<tbody>
[for(cor : Course | aContainer.courses)] 
[for(cal : ExaminationCall| cor.calls)]

<tr>
<td> 
[cal.course.code/]
</td>
<td>
[cal.date/]
</td>
</tr>

[/for]
[/for]
</tbody>
</table>
</p>

[comment ------------------------------------  /]
<p>
<h2>List of Professors</h2>
<table class="table table-bordered">
<thead>
<tr>
<th>Professor name</th>
<th>URL</th>
</tr>
</thead>
<tbody>
[for(pro :Professor | aContainer.professors)] 
[pro.generateProfessor()/]
<tr>
<td> 
[pro.name/] [pro.surname/]
</td>
<td>
<a href="professors/[pro.name/]-[pro.surname/].html">link</a>
</td>
</tr>
[/for]
</tbody>
</table>
</p>
[comment ------------------------------------  /]
<p>
<h2>List of News</h2>
<table class="table table-bordered">
<thead>
<tr>
<th>News title</th>
<th>URL</th>
</tr>
</thead>
<tbody>
[for(pro :Professor | aContainer.professors)] 
[for(news:News      | pro.news_posted)]
[news.generateNews() /]
<tr>
<td> 
[news.title /]
</td>
</tr>
[/for]
[/for]
</tbody>
</table>
</p>


[comment ------------------------------------  /]
<p>
<h2>List of Students</h2>
<table class="table table-bordered">
<thead>
<tr>
<th>Student name</th>
<th>URL</th>
</tr>
</thead>
<tbody>
[for(stu : Student | aContainer.students)] 
<tr>
<td> 
[stu.name/]
</td>
<td>
<a href="[stu.name/].html">link</a>
</td>
</tr>
[/for]
</tbody>
</table>
</p>

[comment ------------------------------------  /]
<p>
<h2>List of CustomEnumeration</h2>
<table class="table table-bordered">
<thead>
<tr>
<th>Enum name</th>
<th>URL</th>
</tr>
</thead>
<tbody>
[for(cus : CustomEnumeration | aContainer.custom_enumerations)] 
<tr>
<td> 
[cus.name/]
</td>
<td>
<a href="[cus.name/].html">link</a>
</td>
</tr>
[/for]
</tbody>
</table>
</p>

[comment ------------------------------------  /]
<p>
<h2>List of ExtraInfo</h2>
<table class="table table-bordered">
<thead>
<tr>
<th>Info name</th>
<th>URL</th>
</tr>
</thead>
<tbody>
[for(inf : ExtraInfo | aContainer.extra_infos)] 
<tr>
<td> 
[inf.name/]
</td>
<td>
<a href="[inf.name/].html">link</a>
</td>
</tr>
[/for]
</tbody>
</table>
</p>
</main>
</body>
</html>
[/file]
[/template]
