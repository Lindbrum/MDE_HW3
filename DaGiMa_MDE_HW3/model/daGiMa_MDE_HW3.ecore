<?xml version="1.0" encoding="UTF-8"?>
<ecore:EPackage xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:ecore="http://www.eclipse.org/emf/2002/Ecore" name="daGiMa_MDE_HW3" nsURI="http://www.example.org/dagima_mde_hw3"
    nsPrefix="dagima_mde_hw3">
  <eClassifiers xsi:type="ecore:EClass" name="University">
    <eStructuralFeatures xsi:type="ecore:EReference" name="rector" lowerBound="1"
        eType="#//Professor"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="departments" unique="false"
        lowerBound="1" upperBound="-1" eType="#//Department" containment="true" eOpposite="#//Department/university"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="name" lowerBound="1" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"
        defaultValueLiteral="A university" iD="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Professor" eSuperTypes="#//User">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="office_telephone" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
