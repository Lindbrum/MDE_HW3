        eType="#//Student" eOpposite="#//Student/thesis_defended"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="supervisor" lowerBound="1"
        eType="#//Professor" eOpposite="#//Professor/supervised_thesis"/>
